{
    "parser": "babel-eslint",
    "settings": {
        "react": {
            "pragma": "React",
            "version": "16.6"
        }
    },
    "env": {
        "browser": true,
        "amd": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-unused-styles": true,
        "no-color-literals": true
    }
}
